<Window x:Class="ParserRobot.UI.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:ParserRobot.UI.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParserRobot.UI.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="700" 
        WindowStartupLocation="CenterScreen">
    <Window.DataContext>
        <vm:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <Button x:Name="startButton" Content="Start" Command="{Binding OnClickStartButton}" Width="400" Height="150" FontSize="40" Background="OrangeRed" Cursor="Hand"/>
    </Grid>
</Window>
